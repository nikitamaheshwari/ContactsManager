<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hp.contactsmanager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.hp.contactsmanager.menu_contactlist" />
        <activity android:name="com.example.hp.contactsmanager.create_contactlist" />
        <activity android:name="com.example.hp.contactsmanager.show_data"/>
        <activity android:name="com.example.hp.contactsmanager.updte" />

    </application>

</manifest>
